@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="product-grid bestsellers tab_wrap">
        <ul class="nav nav-tabs">
            <li class="active" style="float: none; text-align: center;">
            <a data-toggle="tab" href="#bs_first_tab64"> <h2>@T("Bestsellers")</h2></a></li>
        </ul>
         
        <div class="item-grid tab-content">
            @foreach (var item in Model)
            {
                <div class="item-box">
                    @Html.Partial("_ProductBox", item)
                </div>
            }
        </div>
    </div>
}